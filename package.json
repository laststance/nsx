  "scripts": {
    "start": "vite",
    "start:admin": "bash scripts/switch_env admin && vite",
    "start:visitor": "bash scripts/switch_env visitor && vite",
    "build": "bash scripts/switch_env prod && vite build",
    "serve": "vite preview",
    "test": "bash scripts/switch_env test && jest",
    "env": "bash scripts/switch_env",
    "lint": "eslint . --ext .ts,.tsx,.js,jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,jsx --fix",
    "typecheck": "tsc --noEmit",
    "all:typecheck": "yarn typecheck && yarn server:typecheck",
    "server:typecheck": "tsc --noEmit -p server/tsconfig.json",
    "clean": "rimraf yarn.lock node_modules build",
    "format": "prettier --write src/**/*.css .github/**/*.yml package.json .eslintrc index.html tsconfig.json README.md",
    "storybook": "bash scripts/switch_env storybook && start-storybook -p 6006 -s src",
    "build-storybook": "build-storybook -s src",
    "server:start": "cross-env NODE_ENV=development nodemon --ext ts,js --watch 'server/index.ts' --ignore './src' --exec ts-node --files --project server/tsconfig.json server/index.ts",
    "server:build": "tsc -p server/tsconfig.json && cp -r server/db/migrations server_build && cp server/db/config.json server_build/server/db/config.json",
    "deploy": "bash scripts/deploy",
    "workflow:deploy": "yarn build && yarn server:build && yarn deploy",
    "validate": "./scripts/validate",
    "db:hashgen": "node server/db/scripts/passhashgen.js",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed:all",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "cy:open": "cypress open",
    "e2e:admin": "cypress run --headed --spec cypress/integration/admin_basic.js",
    "e2e:visitor": "cypress run --headed --spec cypress/integration/visitor_basic.js",
    "e2e:admin:firefox": "cypress run --headed -b firefox --spec cypress/integration/admin_basic.js",
    "e2e:visitor:firefox": "cypress run --headed -b firefox --spec cypress/integration/visitor_basic.js"
  }
