  "scripts": {
    "postinstall": "pnpm prisma generate",
    "start": "VITE_CJS_IGNORE_WARNING=true vite",
    "dev": "VITE_CJS_IGNORE_WARNING=true vite dev",
    "build": "VITE_CJS_IGNORE_WARNING=true dotenv -e .env.prod vite build",
    "build:e2e": "VITE_CJS_IGNORE_WARNING=true cross-env NODE_ENV=test dotenv -e .env.e2e vite build",
    "preview": "VITE_CJS_IGNORE_WARNING=true vite preview",
    "test": "dotenv -e .env.test -- vitest run",
    "lint": "eslint . -c eslint.config.mjs --concurrency=auto --max-warnings=0",
    "lint:fix": "eslint . -c eslint.config.mjs --fix --concurrency=auto --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf pnpm-lock.yaml node_modules build server_build storybook-static",
    "prettier": "prettier --ignore-unknown --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve:storybook": "serve storybook-static",
    "server:start": "nodemon",
    "server:build": "esbuild server/index.ts --bundle --platform=node --target=node20 --outfile=server_build/server/index.js --format=cjs --packages=external --banner:js=\"const __importMetaUrl = require('url').pathToFileURL(__filename).href;\" --define:import.meta.url=__importMetaUrl",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky",
    "deploy": "bash scripts/deploy",
    "workflow:deploy": "pnpm build && pnpm server:build && pnpm deploy",
    "validate": "./scripts/validate",
    "show:api:docs": "redocly preview-docs openapi_v3.json",
    "db:hashgen": "node server/db/scripts/passhashgen.js",
    "db:reset": "prisma migrate reset --force && prisma db seed",
    "db:truncate": "prisma migrate reset --force",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "playwright": "playwright test",
    "playwright:headed": "playwright test --headed",
    "playwright:ui": "playwright test --ui",
    "playwright:debug": "playwright test --debug",
    "playwright:codegen": "playwright codegen",
    "knip": "knip"
  },
  "pnpm": {
    "overrides": {
      "@wxt-dev/browser": "0.1.4",
